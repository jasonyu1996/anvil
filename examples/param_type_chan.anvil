chan ch<t : type> = {
    left req : (t@#2)
}

proc sub<n : int>(e : left ch<logic[n]>) =
    loop {
        let v = recv e.req in
        v => dprint "Value = %d" (v);
        cycle 2
    }

proc param_type_chan() =
    chan e_le -- e_ri : ch<logic[4]>
    spawn sub<4>(e_le)
    loop {
        send e_ri.req(4'd3) =>
        cycle 2
    }
