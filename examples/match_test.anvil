Use l = 2
Use p = 32
func add (a,b) = 
(
    if(b==2'd0) then a+2'd1
    else
    (
        a+b
    )
)
proc match_test () =
    reg cycle_counter : logic[l]

    loop{
        set cycle_counter := call add (*cycle_counter, 2'd0);
        cycle 1
    }
    loop{
        match *cycle_counter with:
        |> 2'd0 -> dprint "This is first cycle" ()
        |> 2'd1 -> dprint "This is the second cycle" ()
        |> _ -> dprint "More than 2 cycles have passed" ()
        done;
        cycle 1
    }